{
  "manifest_version": 2,
  "name": "SPC",
  "description": "Inserting special characters made easy.",
  "version": "1.0.0",
  "permissions": ["storage"],
  "background": {
    "scripts": ["build/background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": ["build/injection.js"]
    }
  ]
}
